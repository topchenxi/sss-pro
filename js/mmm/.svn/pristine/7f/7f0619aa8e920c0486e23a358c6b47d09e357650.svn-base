<% for list in result %>
  <li class="item-content J_single_seller" data-index="<$ loop.index-1 $>">
    <div class="item-media">
      <!-- <img src="<$ list.imgPath $><$ list.headUrl $>"> -->
    </div>
    <div class="item-inner">
      <div class="item-title-row">
        <div class="item-title">
          <span class="company-name"> 档口名称：<$ list.company $><img src="../../images/pages/buyerOrSellerList/success.png" style="width:25px;height:25px;" /></span>
        </div>
      </div>
      <div class="item-title-row">
        <div class="item-title">
          <span class="company-name"> <$ list.tel $></span>
        </div>
      </div>
      <div class="item-title-row">
        <div class="item-title">
          <span class="company-name"><$ list.province $><$ list.city $><$ list.area $><$ list.addr $></span>
        </div>
      </div>
      <% if list.purchaserRealName %>
      <div class="item-title-row">
        <div class="item-title">
          <span class="company-name">买货员：<$ list.purchaserRealName $></span>
        </div>
      </div>
      <% endif %>
      <% if list.contact %>
      <div class="item-title-row">
        <div class="item-title">
          <span class="company-name">商家联系人：<$ list.contact $>--<$ list.contactTel $></span>
        </div>
      </div>
      <% endif %>
      <!-- <div class="item-subtitle">
          <$ list.tel $>
      </div>
      <div class="item-subtitle">
          <$ list.province $><$ list.province $><$ list.city $><$ list.area $><$ list.addr $>
      </div> -->
    </div>
    <!-- <% if list.purchaserRealName %>
    <div class="verify">
        <img src="images/pages/verify.png" alt="">
    </div>
    <% endif %> -->
  </li>
<% endfor %>
<% if noneData %>
  <div class='none-data'>没有找到相应的供应商哦请添加供应商</div>
<% endif%>
