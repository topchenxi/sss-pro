<% if page.result.length  %>
<% for item in page.result %>
<div class="card money-list-card">
    <div class="card-header">
        <span class='number'>
            订单号：<span style="font-size: 12px;"><$ item.number $></span>
        </span>
        <span class='status'>
        <$ item.orderStatus $>
        </span>
    </div>
    <div class="card-content" >
        <a
            class="block item-link item-content J_jump_to_detail"
             href="/jbOrderDetail.html?id=<$ item.id $>">
            <div class="card-content-inner ">
                <div class='pull-left img'>
                <% if item.iconImg != '' %>
                <img src="http://www.soouya.com<$ item.iconImg $>@100h_100w" style="height: 100%;" alt="">
                <% else %>
                <img src="http://www.soouya.com/upload/util/default_buy.jpg@80h_80w" style="width: 100%; height: 100%;">
                <% endif %>
                </div>
                <ul class='info pull-left'>
                    <li>采购商：<$ item.customerCompany $>  </li>
                    <li>供应商：<$ item.shopCompany $></li>
                    <li><$ item.shopTel $></li>
                    <li><$ item.shopProvince $><$ item.shopCity $><$ item.shopArea $><$ item.shopAddr $></li>
                    <% if item.status == 210 or item.status == 211  or item.status == 510  or item.status == 511 %>
                    <li>成本货款：<span style="color: #f00"><$ item.costMoney $></span>元</li>
                    <% endif %>
                    <% if item.denyRemark != null and item.denyRemark != "" %>
                    <li class="info-deny-remark" title="<$ item.denyRemark $>">不通过备注:<$ item.denyRemark $></nobr></li>
                    <% endif %>
                </ul>
                <span class="icon icon-right pull-right fix-icon"></span>
            </div>
        </a>
    </div>
    <% if item.status == 200 or item.status == 201 or item.status == 502 or item.status == 510 %>
    <div class="card-footers">
        <% if item.status == 200 or item.status == 201 %>
            <a
                href="/jbInputInfo.html?id=<$ item.id $>"
                class="J_list_cache J_jianban_input button button-fill pull-right button-success">
                录入剪版信息
            </a>
        <% endif %>
        <% if item.status == 201 %>
            <a
                href="javascript:;"
                class="button button-fill pull-right button-success J_submit_review"
                data-orderId="<$ item.id $>">
                提交审核
            </a>
        <% endif %>
        <% if item.status == 502 %>
            <a
                href="javascript:;"
                class="button button-fill pull-right button-success J_submit_lading"
                data-orderId="<$ item.id $>">
                确认提货
            </a>
        <% endif %>
        <% if item.status == 510 %>
            <!-- <a
                href="javascript:;"
                class="button button-fill pull-right button-success J_submit_updateTotalMoney"
                data-orderId="<$ item.id $>" data-costMoney="<$ item.costMoney $>">
                编辑金额
            </a> -->
        <% endif %>
    </div>
    <% endif %>
</div>
<% endfor %>
<% else %>
<div class="J_empty_tips" style="text-align: center; margin: 2rem auto"></div>
<% endif %>
