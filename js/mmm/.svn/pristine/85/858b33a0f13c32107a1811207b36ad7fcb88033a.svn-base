.orderList{
    .top-search{
        margin-top: 2.2rem;
        padding: 0 10px;
    }
    .content{
        top: 4.4rem;
    }
    .hurry {
        display: inline-block;
        width: 30px;
        height: 30px;
        line-height: 30px;
        text-align: center;
        background: red;
        border-radius: 50%;
        color: #fff;
    }
    .money-list-card{
        margin: 0.5rem 0;
        .urgent{
            position: absolute;
            right: 3rem;
            top: 1.5rem;
            border: 1px solid red;
            border-radius: 50%;
            width: 1.2rem;
            height: 1.2rem;
            text-align: center;
            line-height: 1.2rem;
            color: #fff;
            font-size: 0.7rem;
            background-color: red;
        }
        .card-header{
            padding: .5rem;
            font-size: 0.6rem;
        }
        .block{
            display: block;
            overflow: hidden;
        }
        .card-content-inner{
            width: 100%;
            display: inline-block;
            overflow: hidden;
            padding: .5rem .8rem;
            .img{
                width: 30%;
                max-width: 3rem;
                max-height: 3rem;
                overflow: hidden;
                background-color: #EFEFEF;
            }
        }
        .top-search{
            padding: 0 10px;
        }
        .info{
            width: 65%;
            max-width: 10.8rem;
            color: #333;
            padding-left: 0.5rem;
            margin-top: 0;
            position: relative;
            li{
                width: 100%;
                overflow: hidden;
                font-size: 0.7rem;
                margin: 0;
                padding: 0;
                color: #999;
            }
            .limitHeight{
                height: 1.0rem;
                overflow: hidden;
                white-space: nowrap;
                text-overflow: ellipsis;
            }
            .haveRealVersion {
                // position: absolute;
                // right: -2.0rem;
                // top: 0;
                color: red;
            }
            .info-deny-remark {
              color: #f00;
              word-break:break-all;
              width: 254px;
              height: 50px;
              overflow: hidden;
              text-overflow: ellipsis;
            }
        }
        .card-footers{
            border-top: 1px solid #D2D2D2;
            min-height: 2.2rem;
            min-height: 0;
            height: 2.2rem;
            padding: 0 .2rem;
            padding-top: .4rem;
            .button.pull-right{
              margin-left: 0.5rem;
            }
            .money-tips{
                font-size: 0.6rem;
            }
        }
        .fix-icon{
            font-size: 0.9rem;
            margin-top: 1.0rem;
            color: #000;
        }
    }
    .infinite-scroll-preloader{
        width: 100%;
        position: absolute;
        bottom: 0;
        text-align: center;
        z-index: 10;
        display: none;
    }
}
