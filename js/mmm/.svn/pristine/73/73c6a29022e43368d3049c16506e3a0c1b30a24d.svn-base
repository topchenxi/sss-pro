<form class="J_form">
<ul class="colorBlock J_bill_section">
  <li class="row no-gutter">
    <div class="col-80 item-content">
      <ul class="item-son-content">
        <li>
            <div class="item-inner">
                <div class="col-22 pull-left" style="padding-top: 10px;margin-right: 5px;">上传凭证</div>
                <div class="common-r">
                  <img src="<$ base64 $>"  class='J_zoom'  style="border:1px solid #ccc; width: 4rem; height: 3.5rem;" />
                  <input value="<$ imgUrl $>" name="imgUrl" id="single<$ randomNumber $>" type="hidden" class="J_images" />
                  <input value="<$ base64 $>" name="base64" type="hidden" />
                </div>
            </div>
        </li>
        <li>
            <div class="item-inner">
                <div class="pull-left col-20" style="padding-top: 9px;">备注</div>
                <div class="common-r  col-80">
                <textarea class="enter J_remark"  name="remark" placeholder="填写备注"></textarea>
                </div>
            </div>
        </li>
        </ul>
    </div>
    <div class="col-20 del warning-text item-link J_del">
      <span>删除</span>
    </div>
  </li>
</ul>
</form>
