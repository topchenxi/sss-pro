<!-- 验布报告上传模板 -->
<ul class="colorBlock J_bill_section">
<li class="row no-gutter">
  <div class="col-80 item-content">   
    <ul class="item-son-content">
      <li> 
          <div class="item-inner">
              <div class="col-22 pull-left" style="padding-top: 10px;margin-right: 5px;">上传凭证</div>
              <div class="common-r">
                <img src="<$ imgUrl $>" alt="" class='J_zoom' width="40" height="40" style="border:1px solid #ccc;" />
              </div>
          </div>
      </li>
      <li> 
          <div class="item-inner">
              <div class="pull-left col-20" style="padding-top: 9px;">匹号</div>
              <div class="common-r  col-80">
              <input class="enter" type="text" placeholder="填写对应匹号"> 
              </div>
          </div>
      </li>
      <li> 
          <div class="item-inner">
              <div class="col-20 pull-left">评价</div>
              <div class="common-r col-80 pull-left">
               <div class="checkbox-wrap">
                  <div class="checkbox assert">
                    <label  class="assert-btn" for="orderMoney<$ randNumber $>">
                       合格
                    </label>
                    <label  class="assert-btn labelOn" for="leftMoney<$ randNumber $>">
                       待沟通
                    </label>
                    <label   class="assert-btn" for="serviceMoney<$ randNumber $>">
                       不合格
                    </label>
                  </div>
                </div>
              </div> 
          </div>
      </li>
      
      <li> 
          <div class="item-inner">
              <div class="pull-left col-20" style="padding-top: 9px;">备注</div>
              <div class="common-r  col-80">
              <input class="enter" type="text" placeholder="填写备注"> 
              </div>
          </div>
      </li>
      </ul>
  </div>
  <div class="col-20 del warning-text item-link J_del" style="height: 11.5rem;">
    <span>删除</span>
  </div>  
</li>
</ul>
                  