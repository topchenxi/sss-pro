<form class="list-block J_big_good">
    <ul>
        <li class="item-content">
          <div class="item-inner">
            <div class="item-title label">*大货类型</div>
            <div class="item-input radio-group">
              <% if type == 1 %>
                <label for="type-check-" >
                  <input type="hidden" name='type' value="1" id='type-check-1'
                  >服务单
                </label>
              <% endif %>
              <% if type == 2 %>
                <label for="type-check-2">
                  <input type="hidden" name='type' value="2" id='type-check-2'>贸易单
                </label>
              <% endif %>
            </div>
          </div>
        </li>
        <li class="item-content">
         <div class="item-inner">
           <div class="item-title label">货源</div>
           <div class="item-input radio-group J_productSource">
