<% if result.length %>
  <% for item in result %>
    <div class="block item-link item-content order-item">
      <div class="card money-list-card">
        <div class="card-header">
            <span class='number'>订单号：<$ item.serviceNumber $></span>
            <span>
              <$ item.redwoodDescr $>
            </span>
        </div>
        <div class="card-content">
          <a class="card-content-inner"  style="position: relative;" href="/orderDetail.html?orderNumber=<$ item.orderNumber $>">
            <div class='pull-left img'>
              <% if item.imgUrls.length > 0 %>
                <img src="<$ imgPath $>/<$ item.imgUrls[0] $>@100h_100w_1e" style="height: 100%;" alt="" class="">
              <% else %>
                <img src="<$ imgPath $>/upload/util/default_buy.jpg@80h_80w_1e" style="width: 100%; height: 100%;" class="">
              <% endif %>
            </div>
            <ul class='info pull-left'>
              <li>采购商： <$ item.buyer.company $>  </li>
              <li>供应商： <% if item.sellerCompany %><$ item.sellerCompany $><% endif %>
              <% if not item.sellerCompany %>----<% endif %></li>
              <% if category == 'zb-all' %>
                <li class="limitHeight"><$ item.wantProductList[0].tagNames $></li>
              <% endif %>
              <% if category == 'jb-all' %>
                <li>电话：<$ item.sellerTel $></li>
                <li>地址：<$ item.sellerProvince $><$ item.sellerCity $><$ item.sellerArea $><$ item.sellerAddr $></li>
              <% endif %>
              <% if category == 'all-all' and listKey == 'ywc' %>
                <li>订单时间：<$ item.createTimeString $></li>
              <% endif %>
            </ul>
            <span class="icon icon-right pull-right fix-icon"></span>
          </a> 
        </div>
        <% if category == 'all-all' and listKey == 'ywc' %>
          <div class="card-footer">
            <span class='money pull-right'>￥<$ item.totalMoney $></span>
            <span class="button button-fill pull-right button-success again J_publish_again"  data-orderNumber='<$ item.orderNumber $>'>再来一单</span>
          </div>
        <% endif %>
      </div>
    </div>
  <% endfor %>
<% else %>
  <div class="J_empty_tips" style="text-align: center; margin: 2rem auto"></div>
<% endif %>
