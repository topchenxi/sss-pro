.upDeliverGood-wrap {
    .has-upload {
        display: inline-block;
        padding: 10px 0 0 10px;
        overflow: hidden;
        .add-img-btn {
              position: relative;
              cursor: pointer;
              display: inline-block;
              width: 4.1rem;
              height: 3.6rem;
              text-align: center;
              vertical-align: top;
              .add-icon {
                    position: absolute;
                    top: -15px;
                    width: 100%;
                    left: 0;
                    text-align: center;
                    font-size: 2rem;
                }
              .text {
                    position: absolute;
                    bottom: 0;
                    width: 100%;
                    left: 0;
                    text-align: center;
              }
              .img-upload-label {
                    display: inline-block;
                    position: absolute;
                    width: 100%;
                    height: 100%;
                    right: 0;
                    top: 0;
                    z-index: 1000;
              }
              .file-input {
                  display: none;
              }

        }
        .where-image{
          float: left;
          .up-img{
            float: left;
            width: 4rem;
            height: 3.5rem;
            border:1px solid #ccc;
            margin-right: 10px;
            margin-bottom: 10px;
            position: relative;
            .del {
              position: absolute;
              width: 20px;
              height: 20px;
              border: 1px solid #ccc;
              text-align: center;
              line-height: 17px;
              border-radius: 50%;
              background: #ccc;
              color: #fff;
              right: -10px;
              top: -8px;
            }
          }
          .zoomImg {
            width: 4rem;
            height: 3.5rem;
          }
          .warning {
            border: 1px solid red;
          }
        }
    }
    .goodSource{
        margin-right: 20px;
    }
    .section {
        margin-bottom: 10px;
    }
    .carInfo{
        display: none;
    }
    .disabled{
      color: #000;
      background-color: #eee;
    }
    .submit{
        position: fixed;
        left: 0;
        bottom: 0;
        right: 0;
    }
}
