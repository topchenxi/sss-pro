.shareClass
{
    padding: 10px 30px;

    background-color: #fff;
    .el-table__expand-icon
    {
         > .el-icon
        {
            margin-top: -8px;
            margin-left: -8px;
        }
        .el-icon-arrow-right
        {
            display: none;
            display: inline-block;

            width: 16px;
            height: 16px;

            background: url(assets/images/add.png) center center no-repeat;
            &:before
            {
                content: '';
            }
        }
        &.el-table__expand-icon--expanded
        {
            transform: rotate(0deg);
            .el-icon-arrow-right
            {
                background: url(assets/images/del.png) center center no-repeat;
            }
        }
    }
    p
    {
        line-height: 35px;
    }
    span
    {
        line-height: 35px;

        display: inline-block;

        height: 35px;
    }
    .show-image
    {
        &:after,
        &:before
        {
            display: table;

            content: '';
        }
        &:after
        {
            clear: both;
        }
        .card
        {
            width: 120px;
            margin-top: -25px;
            margin-left: 20px;
            &:first-child
            {
                margin-left: 0;
            }
        }
        .image
        {
            width: 100%;
            height: 120px;

            cursor: pointer;
        }
        .length-text
        {
            position: relative;
            top: 132px;

            text-align: center;

            border-radius: 0 0 5px 5px;
            background-color: rgba(0, 0, 0, .5);
            p
            {
                line-height: 25px;

                height: 25px;

                color: #f6f6f6;
            }
        }
    }
    .show-image-small
    {
        &:after,
        &:before
        {
            display: table;

            content: '';
        }
        &:after
        {
            clear: both;
        }
        .card
        {
            width: 60px;
            margin-top: -20px;
        }
        .image
        {
            width: 100%;
            height: 50px;

            cursor: pointer;
        }
    }
}
