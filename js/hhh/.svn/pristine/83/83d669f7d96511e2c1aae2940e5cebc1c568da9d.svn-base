.rr-msg
{
    .el-table__expanded-cell
    {
        padding: 10px 20px;

        background-color: #fafafa;
    }
    table
    {
        width: 100%;

        border-collapse: collapse;

        border: none;
        tr
        {
            &:hover
            {
                background-color: transparent;
            }
        }
        th
        {
            font-weight: 600;

            padding: 4px;

            color: #262626;
            border: 1px solid #ddd;
            background-color: transparent;
        }
        td
        {
            padding: 4px;

            word-break: break-all;

            border: 1px solid #ddd;
            background-color: transparent;
        }
    }
}
