.allDetail
{
    width: 80%;
    min-height: 100%;
    margin: auto;
    padding-left: 15px;

    background-color: #f9f9f9;
    .detail-title
    {
        margin-bottom: 20px;
        margin-left: 25px;
        padding-top: 10px;

        @include clearfix;
        .back,
        span
        {
            display: inline-block;
            float: left;

            width: 40px;
            height: 30px;
            margin-left: -16px;

            cursor: pointer;

            background: url('~assets/images/back.png') center center no-repeat;
        }
        p
        {
            font-size: 20px;
            line-height: 30px;

            float: left;

            color: #333;
        }
    }
    .msg-content
    {
        margin-top: 10px;
        margin-bottom: 10px;
        padding: 10px 0;

        background-color: #fff;
    }
}
.allDetail-tab
{
    height: 54px;
    padding: 0 30px;

    border-bottom: 1px solid #e9e9e9;

    @include clearfix;
    li
    {
        font-size: 16px;
        font-weight: bold;
        line-height: 53px;

        display: inline-block;
        float: left;

        height: 54px;
        padding: 0 20px;

        cursor: pointer;
        text-align: center;

        color: #39ab65;
        border-bottom: 2px solid #e9e9e9;
    }
    .isActive
    {
        color: #39ab65;
        border-bottom: 2px solid #39ab65;
    }
}
.order-msg
{
    .status-spans
    {
        height: auto;
        margin: 10px 0 13px 0 ;

        @include clearfix;
        span
        {
            font-size: 12px;
            line-height: 20px;

            display: inline-block;
            float: left;

            height: 20px;
            padding: 0 8px;

            color: #2fb468;
            border: 1px solid #2fb468;
            border-radius: 4px;
            background: rgba(47,180,104,.10);
             + span
            {
                margin-left: 8px;
            }
            &.check
            {
                color: #46a7ff;
                border: 1px solid #46a7ff;
                border-radius: 4px;
                background: rgba(70,167,255,.10);
            }
            &.sendType
            {
                color: #fb774a;
                border: 1px solid #fb774a;
                border-radius: 4px;
                background: rgba(251,119,74,.10);
            }
        }
    }
    .money-content
    {
        min-height: 128px;
        margin-top: 15px;
        padding: 20px 0 0 0;

        border: 1px solid #e9e9e9;
        border-radius: 3px;
        .el-row:first-child
        {
            margin-bottom: 5px;
            padding-bottom: 10px;
        }
        .label
        {
            font-size: 12px;
            line-height: 18px;

            height: 18px;

            color: rgba(0,0,0,.45);
        }
        .size16
        {
            font-size: 16px;
        }
        .edu-label
        {
            font-size: 12px;
            line-height: 16px;

            height: 16px;

            color: rgba(0,0,0,.65);
        }
        .bg-yue
        {
            line-height: 48px;

            display: inline-block;

            width: 48px;
            height: 48px;
            margin-left: 15px;

            text-align: center;

            color: #fff;
            border-radius: 50%;
            background-color: #f88866;
        }
        .bg-baitiao
        {
            line-height: 48px;

            display: inline-block;

            width: 48px;
            height: 48px;
            margin-left: 15px;

            text-align: center;

            color: #fff;
            border-radius: 50%;
            background-color: #2fb468;
        }
        .bg-yanzhen
        {
            line-height: 48px;

            display: inline-block;

            width: 48px;
            height: 48px;
            margin-left: 15px;

            text-align: center;

            color: #fff;
