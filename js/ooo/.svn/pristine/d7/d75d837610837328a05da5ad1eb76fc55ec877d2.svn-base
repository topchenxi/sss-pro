.el-bulkTable-content {
  padding-top: 12px;
  background-color: #fff;
  .el-table {
    .el-table__header-wrapper {
      text-align: center !important;
    }
    text-align: center;
    th {
      text-align: center;
      background-color: #f9f9f9;
      border-color: #ddd;
      padding: 5px !important;
      .cell {
        padding: 0 5px;
        background-color: #f9f9f9;
      }
    }
    td {
      border-color: #ddd;
      padding: 5px 0 !important;
      .cell {
        padding: 0 5px;
      }
    }
  }
}

.bulkMsg-content {
  margin-left: 5px;
  margin-right: 15px;
  background-color: #fff;
  border-top: 2px solid #39ab65;
  padding: 10px 15px;
  margin-bottom: 15px;
  color: #333;
  .msg-title {
    font-size: 16px;
    font-weight: 600;
  }
  p {
    line-height: 35px;
    font-size: 14px;
  }
  span {
    font-size: 14px;
    display: inline-block;
    height: 35px;
    line-height: 35px;
  }
  .bottom-border-dashed {
    margin-top: 5px;
  }
  .f9-bg {
    padding: 8px 15px;
    background-color: #f9f9f9;
  }
}
.el-bulkCollapse-content {
  padding-top: 12px;
  background-color: #fff;
  overflow: scroll;
  .fold-content {
    padding: 5px 15px;
    border: 1px solid #ddd;
    border-radius: 5px;
    margin-bottom: 10px;
    background-color: #f9f9f9;
    cursor: pointer;
    span {
      display: inline-block;
      line-height: 25px;
      height: 25px;
    }
    .icon-daicai {
      background-color: #FA9D3B;
      border-radius: 8px;
      height: 20px;
      line-height: 18px;
      color: #fff;
      padding: 1px 3px;
    }
    .el-icon-arrow-right {
      float: right;
      margin-top: 25px;
      clear: both;
    }
    .el-icon-arrow-down {
      float: right;
      margin-top: 25px;
      clear: both;
    }
    .fold-content-wrap {
      width: 100%;
      background-color: #fff;
      .clothLone-content {
        padding: 5px 15px;
        border-bottom: 1px dashed #ddd;
        &:last-child {
          border-bottom: 0px;
        }
      }
      .icon-ff {
        background-color: #39ab65;
        width: 8px;
        height: 8px;
        border-radius: 4px;
        vertical-align: middle;
        margin-right: 0px;
      }
      .left-margin15 {
        margin-left: 15px;
      }
    }
    .fade-enter-active {
      animation-name: fade-open;
      animation-duration: 0.5s
    }
    .fade-leave-active {
      animation-name: fade-close;
      animation-duration: 0.5s;
    }
    @keyframes fade-open {
      0% {
        height: 0%;
        width: 100%;
      }
      100% {
        height: 100%;
        width: 100%;
      }
    }
    @keyframes fade-close {
      0% {
        height: 100%;
        width: 100%
      }
      100% {
        height: 0%;
        width: 100%;
      }
    }
  }
}
